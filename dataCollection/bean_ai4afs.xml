<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><h:head><h:title>Common Bean</h:title><model odk:xforms-version="1.0.0"><submission action="https://docs.google.com/spreadsheets/d/1wk_uir_BtvGgMg/edit#gid=0" method="post"/><instance><data id="beans"><fid><start/><end/><today/><deviceid/><imgtype/><imgtype_other/><img/><obs/><gps/></fid><meta><instanceID/></meta></data></instance><bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/fid/start" type="dateTime"/><bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/fid/end" type="dateTime"/><bind jr:preload="date" jr:preloadParams="today" nodeset="/data/fid/today" type="date"/><bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/fid/deviceid" type="string"/><bind nodeset="/data/fid/imgtype" required="true()" type="string"/><bind nodeset="/data/fid/imgtype_other" relevant="selected(../imgtype, 'other')" type="string"/><bind nodeset="/data/fid/img" orx:max-pixels="1000" required="true()" type="binary"/><bind nodeset="/data/fid/obs" type="string"/><bind nodeset="/data/fid/gps" required="true()" type="geopoint"/><bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/></model></h:head><h:body><group appearance="field-list" ref="/data/fid"><label>Common Bean</label><select1 ref="/data/fid/imgtype"><label>Type of Picture</label><item><label>Healthy</label><value>healthy</value></item><item><label>Bean rust</label><value>rust</value></item><item><label>Bean anthracnose</label><value>anthra</value></item><item><label>Other</label><value>other</value></item></select1><input ref="/data/fid/imgtype_other"><label>Specify other.</label></input><upload mediatype="image/*" ref="/data/fid/img"><label>Leaf Photo</label><hint>Take Leaf Picture</hint></upload><input ref="/data/fid/obs"><label>Comment</label><hint>Specify any observations (e.g. variety)</hint></input><input ref="/data/fid/gps"><label>Location</label><hint>Please wait until phone gets reading</hint></input></group></h:body></h:html>